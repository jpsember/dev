# Whitespace is skipped (EXCEPT newlines)
SPACES: [\f\s\t]+
CR: \n
COMMENT: ($_HASH | //)  [^\n]* \n?
NUMBER: $_UNSIGNEDINT
EQUALS: $_EQUALS
ERRCODE: ERRCODE\_ $_ID
PUBLIC: public
STATIC: static
FINAL: final
INT: int
SEMICOLON: \;
COLON: $_COLON
PUB: pub
CONST: const
USIZE: usize



